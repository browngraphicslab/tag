	width 25%

#newCatalogRoot #selectedArtworkContainer
	background-color rgba(0,0,0,0.65)
	width 32%
	height 85%
	z-index 10000
	position absolute
	display none
	border-radius 2%

#newCatalogRoot .tile
	border 1px solid rgba(0,0,0,0.85)
	position absolute
	height 45%
	width 33% 
	border-radius 3%	

#newCatalogRoot .tileImage
	position absolute
	wh 100%
	border-radius 3%	

#newCatalogRoot #timelineArea
	position absolute
	width 100%
	height 85%
	bottom 0

#newCatalogRoot .timeline
	position absolute
	width 100%
	height 3%
	top 45%

#newCatalogRoot .timelineTick
	position absolute
