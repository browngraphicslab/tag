@import 'common.styl'

#newCatalogRoot
	background-color rgb(127,127,127)
	overflow-x hidden
	position absolute
	wh 100%

#newCatalogRoot #artistInfo
	color white
	float left
	position relative
	overflow hidden          
	text-overflow ellipsis
	white-space nowrap
	width 70%

#newCatalogRoot .artText
	margin-top 0.84%
	font-size 0.75em
	overflow hidden
	text-align center         
	text-overflow ellipsis                
	white-space nowrap
	width 90%
	position relative
	left 5%

#newCatalogRoot .artTitle
	background-color rgba(0, 0, 0, 0.6)
	height 20%
	opacity 1.0
	position absolute
	width 100%
                
#newCatalogRoot #backbuttonArea
	left 1%
	position absolute
	width 3%

#newCatalogRoot	#bgimage
	background-position center
	background-size cover
	opacity 0.40
	position absolute
	wh 100%

#newCatalogRoot #bottomContainer
	width 100%
	height 70%
	position absolute
	bottom 0

#newCatalogRoot #catalogBackButton
	display block
	height auto
	position relative
	top 0%
	wh 100%

#newCatalogRoot #collectionDescription
	width 90%
	height 95%
	overflow-y auto
	margin 5%
	margin-top 0%

#newCatalogRoot #currentThumbnail
	wh 100%
	border-radius 2%

#newCatalogRoot #descSpan
	text-overflow ellipsis
	word-wrap break-word

#newCatalogRoot #description-text
	color white
	overflow-x hidden
	overflow-y auto
	white-space normal
	word-wrap break-word
	position absolute
	width 90%
	height 25%
	margin 5%
	bottom 0

#newCatalogRoot #infoDiv
	position absolute
	left 0%
	height 90%
	width 25%

#newCatalogRoot #displayHelp
	background-color rgba(0,0,0,0.5)
	height 100%
	left 0%
	position relative
	top 0.5%
	width 100%   

#newCatalogRoot #displayHelpText
	font-size 2.5em
	margin 0px auto
	position relative
	text-align center
	width 65%

#newCatalogRoot #displayHelpTitle
	font-size 2em 
	margin 0px auto
	padding-top 15%
	position relative           
	text-align center
	width 60%

#newCatalogRoot #collectionArea
	background-color rgba(0, 0, 0, 0.7)	
	height 20%
	overflow-x auto
	overflow-y hidden
	width 92% 
	position absolute
	left 6%

#newCatalogRoot #collection-label
	height 7%
	left 19%
	margin-left 1.22%
	padding 0px 0px 0px 10px
	position absolute
	top 2.45%
	text-align center
	width 45%   
	font-size 1.7em

#newCatalogRoot #collection-selection
	border solid transparent
	border-width 5px 0px 5px 0px
	display inline-block
	height 9.6%
	position relative
	padding-bottom 0.75%
	overflow none
	width 100%

#newCatalogRoot .collection-title
	display inline-block
	height 100%
	margin-left 5%
	overflow hidden
	text-overflow ellipsis
	white-space nowrap
	width 90%
	font-size 1.1em
	margin-top 2.15%

#newCatalogRoot #exploreIcon
	float right
	height 50%
	margin-right 32%
	position relative
	top 25%
	width auto

#newCatalogRoot #explore-tab
	background-color rgba(0,0,0,0.4)
	color white
	display none
	height 16%
	overflow hidden 
	position absolute
	left 0
	width 100%  

#newCatalogRoot #explore-text
	float right
	margin-right 3% 
	position relative
	top 8%         

#newCatalogRoot #feedbackContainer
	bottom 0%
	float right
	height auto
	position relative
	padding-top 5.25%        
	right 1.5%
	width 6%

#newCatalogRoot #feedback-icon
	height 38%
	position absolute
	right 5%
	top 10%
	vertical-align middle
	width auto

#newCatalogRoot #img-container
	width 90%
	height 50%
	position absolute
	top 10%
	left 5%

#newCatalogRoot #info-text
	display inline-block
	float top
	top 100%        
	width 100% 

#newCatalogRoot #topBar
	height 27.5%
	position absolute
	width 100%
	top 2.5%
	z-index 2

#newCatalogRoot #topBar-header
	height 5%
	margin-bottom 15%
	overflow hide
	width 100%

#newCatalogRoot #loadingArea
	background-color rgba(0, 0, 0, 0.7)
	height 100%
	position absolute
	width 100% 
	z-index 1000000

#newCatalogRoot #overlay
	background-color rgba(0, 0, 0, 0.7)
	position absolute
	wh 100%

#newCatalogRoot #sortRow
	height 10%
	position relative
	top 0%
	width 100%
	z-index 1000

#newCatalogRoot .rowButton
	cursor pointer
	float left
	font-size 92.5%
	margin 0.475% 2%

#newCatalogRoot #searchInput
	background-color white
	border-color rgb(167,180,174)
	border-style solid
	border-width 0.05em
	color rgb(120,128,119)
	float right
	font-size 80%
	max-width 15%
	margin-top 0.5%
	position relative
	right 1%
	z-index 100
	border-radius 2%


#newCatalogRoot #searchTxt
	color white
	float right
	font-size 80%
	margin 0.65% 3% 0 0
