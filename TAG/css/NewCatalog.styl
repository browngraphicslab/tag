	text-align center
	text-overflow ellipsis
	overflow hidden
	white-space nowrap
	width 95%


#newCatalogRoot .tourLabel
	bottom 0px
	margin-right -7%
	position absolute
	right 5px
	wh 30%

#newCatalogRoot .videoLabel
	bottom 0px
	height 35%
	position absolute
	right 5px
	width 20%

#newCatalogRoot #yearInfo
